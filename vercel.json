{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(?:css|js|png|jpg|jpeg|gif|ico|svg))",
      "dest": "/api/public/$1"
    },
    {
      "src": "/swagger.yaml",
      "dest": "/api/public/swagger.yaml"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}